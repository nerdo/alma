{
    "source": "./src",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "objectRestSpread": true
            }
        },
        {
            "name": "esdoc-standard-plugin"
        }
    ],
    "brand": {
        "title": "Alma",
        "description": "A class-based implementation of the SAM pattern (sam.js.org).",
        "repository": "https://github.com/nerdo/alma.git",
        "author": "Dannel Albert"
    }
}
